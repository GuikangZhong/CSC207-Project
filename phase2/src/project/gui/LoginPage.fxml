<AnchorPane prefHeight="200.0" prefWidth="300.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.gui.LoginController">
   <children>
      <VBox alignment="CENTER" layoutX="77.0" layoutY="22.0" prefHeight="120.0" prefWidth="146.0" spacing="10.0" AnchorPane.bottomAnchor="58.0" AnchorPane.leftAnchor="60.0" AnchorPane.rightAnchor="60.0" AnchorPane.topAnchor="22.0">
         <children>
            <TextField fx:id="usernameInput" prefHeight="20.0" prefWidth="100.0" promptText="username" />
            <TextField fx:id="passwordInput" prefHeight="25.0" prefWidth="100.0" promptText="password" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
