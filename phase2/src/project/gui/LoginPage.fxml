         <children>
            <Button alignment="CENTER" mnemonicParsing="false" onAction="#loginButton" prefWidth="80.0" text="login" />
            <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#returnButton" prefWidth="80.0" text="return" HBox.hgrow="ALWAYS" />
